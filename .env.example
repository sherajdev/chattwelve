# ChatTwelve Environment Variables

# MCP Server URL (TwelveData MCP server)
MCP_SERVER_URL=http://localhost:3847

# OpenRouter API Key (required for AI features)
# Get your key at: https://openrouter.ai/keys
OPENROUTER_API_KEY=

# AI Models (optional - defaults shown)
AI_PRIMARY_MODEL=openai/gpt-5.2
AI_FALLBACK_MODEL=google/gemini-3-flash-preview

# AI Mode (optional - defaults to true)
# true = Use AI agent with autonomous tool calling
# false = Use manual routing with query processor
USE_AI_AGENT=true

# Debug mode
DEBUG=false
